{
  "extends": "airbnb/base",
  "parser": "babel-eslint",

  "plugins": [],

  "env": {},

  "globals": {
    "_": true,
    "require":true,
    "modules":true,
    "process":true,
    "describe":true,
    "it":true,
    "after":true,
    "before":true
  },

  "rules": {
    "no-plusplus": 0,
    "no-multi-spaces": 0,
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "maximumLineLength": 0,
    "arrow-body-style": 0
  }
}
